<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"><shape android:shape="oval">
        <solid android:color="#ff3276b1" />
        <stroke android:width="1dp" android:color="#ff357ebd" />
        <size
            android:width="20dp"
            android:height="20dp" />
    </shape></item>

    <item android:state_enabled="false"><shape android:shape="oval">
        <solid android:color="#d4e5f3" />
        <stroke android:width="1dp" android:color="#a5357ebd" />
        <size
            android:width="20dp"
            android:height="20dp" />
    </shape></item>


    <item><shape android:shape="oval">
        <solid android:color="#ff428bca" />
        <stroke android:width="1dp" android:color="#ff357ebd" />
        <size
            android:width="20dp"
            android:height="20dp" />
    </shape></item>
</selector>